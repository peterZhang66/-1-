.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 33.33%;  
    color: #777777;  
}  
.on{ color: #da7c0c;  
    border-bottom: 5rpx solid #da7c0c;}  
  
.swiper-box{ display: block; height:1334rpx; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: center;  
}  
page{position:fixed}