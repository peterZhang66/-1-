/* pages/list/list.wxss */
.search{
    
     align-items: center;
      padding:25rpx 14rpx;
}
.search>view{
      border:1px solid #dfdfdf;
      height:70rpx;
      border-radius: 5px;
      display: flex;
      align-items: center;
      overflow: hidden;
     
}
.search>view icon{
      margin-left:10px;
}
.search>view input{
      flex:1;
      margin-left:10px;
}
.top-btn-box{
      display: flex;
      border-top:1px solid #dfdfdf;
       border-bottom:1px solid #dfdfdf;
}
.top-btn-box .item{
      flex:1;
      text-align: center;
      height:80rpx;
      line-height: 80rpx;
      color:#999;
}
.list-content{
      background:#F5F5F5;
      display: flex;
      height: 100%
}
.list-left{
      width:25%;
      height:100%;
}
.list-left .view{
      text-align: center;
      padding:20rpx 0;
      color:#999;
      font-size: 11pt;
      border-bottom:1px solid #dfdfdf;
}
.list-right{
      background:#fff;
      flex:1;
      padding:15rpx;
      height: 85%
     
}
.list-right .item{
  float:left;
  width:50%;
 margin-bottom:10rpx;
}
.list-right .item .cont{
   width:250rpx;
   border:1px solid #dfdfdf;
   margin:0 auto;
}
.list-right .item image{
   width:200rpx;
   height:200rpx;
   display: block;
   margin:5px auto;
}
.list-right .item .cont .text{
      display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
margin: 10rpx;
font-size: 10pt;
color:#666;
}
.cont-foot{
      margin: 10rpx;
       text-align: center
}
.cont-foot .item{
   width:100%;
 
}
.money{
      color:#FE354D;
      font-size: 11pt;
}
.money text{
      font-size: 9pt;
}
.shopping{
      font-size: 9pt;
      color:#999;
}
